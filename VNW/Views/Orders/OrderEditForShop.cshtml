                            '<tr class="tr_newP ' + pid + '">'
                            + '<td><span name="OdsRow">' + (OdsRowCount + 1) + '</span> <b class="text-danger">NEW</b> '
                            + '<div class="btn_removeItem btn btn-default" pid="' + pid + '"><img src="../../images/delete.svg"/></div>' + '</td><td>'
                            + image + '</td><td>'
                            + pidInput + ' '
                            + nameInput + '</td><td>'
                            + qtySelect + '</td><td>'
                            + price + '</td><td>'
                            + totalPrice + '</td><td></td><td>'
                            + data.stock + '</td><td>'
